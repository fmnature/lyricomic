<div class="scroll" data-max="20" data-lastnum="2">
<% notifications = @notifications.where.not(sender_id: current_user.id) %>
<div class="contents">
  <% if notifications.exists? %>
    <% notifications.each do |notification| %>
      <%= render "notifications", notification: notification%>
    <% end %>
  <% else %>
    <p>通知はありません</p>
  <% end %>
</div>
</div>